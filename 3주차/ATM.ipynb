    "# https://www.acmicpc.net/problem/11399\r\n",
    "\r\n",
    "num = input()\r\n",
    "timeArr = list(map(int, input().split()))\r\n",
    "time, wait = 0, 0\r\n",
    "# 시간순으로 정렬\r\n",
    "timeArr.sort()\r\n",
    "for i in timeArr:\r\n",
    "    time += i + wait\r\n",
    "    wait += i\r\n",
    "\r\n",
    "print(time)"
