      "source": [
        "def toggleCase(str):\n",
        "  result = \"\"\n",
        "  for ch in str:\n",
        "    result += chr(ord(ch) ^ 32)\n",
        "  return result\n",
        "\n",
        "str = \"ChErRy\"\n",
        "print(\"Original string: \" + str)\n",
        "print(\"toggleCase: \" + toggleCase(str))"
      ],
      "execution_count": 32,
